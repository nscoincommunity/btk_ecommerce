<table class="table table-hover">
      <thead>
        <tr>
          <th>#</th>
          <th>File name</th>
          <th>Date</th>
        </tr>
      </thead>
      <tbody>
        {% set i=0 %}
        {% for file in files %}
          <tr>
            <td> {{ i }}</td>
            <td> {{ file_names[i] }} </td>
            <td> {{ file_created_dates[i] }} </td>
          </tr>
      {% set i=i + 1 %}
      {% endfor %}
      </tbody>
    </table>
